package aula1;

public class exemplo3 {

	public static void main(String[] args) {
		// escopo de variáveis
		
		//boa pratica
		int valorA = 10, valorB = 30;
		
		//int valorA = 10;
		//int valorB = 20;
		
		//int resultado = valorA + valorB; 
		
		System.out.println("Soma: " + (valorA + valorB));
		System.out.println("Subtração: " + (valorA - valorB));
		
		
		
	}
}
