package aula1;

public class exemplo4 {

	public static void main(String[] args) {
		//caixinhas
		
		int a=30;
		int b=30;
		int soma;
		int divisao;
		int subtracao;
		int mult; 
		
		soma = a+b;
		subtracao= a-b;
		mult= a*b;
		divisao = a/b;
		
		System.out.println(soma);
		System.out.println(subtracao);
		System.out.println(mult);
		System.out.println(divisao);
	}

}
