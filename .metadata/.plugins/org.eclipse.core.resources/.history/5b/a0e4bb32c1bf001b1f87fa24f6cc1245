package aula3;

public class Livros {
	
	String nome;
	String autor;
	String genero;
	String tipoDeMidia;
	String anoDePublicacao;
	String editora;
	String prefacio;
	String idioma;
	String isbn;
	String edicao;
	
	
	void exibirLivro() {
		System.out.println(nome);
		System.out.println();
		System.out.println();
		System.out.println();
		System.out.println();
		System.out.println();
	}
	
}
