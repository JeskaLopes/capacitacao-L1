package aula3;

public class ContaCorrente {
	
	//atributos
	String nome;
	String numeroConta;
	String agencia;
	String pix;
	String saldo;
	
	//métodos
	void extrato() {
		System.out.println("Nome da conta: ");
		System.out.println("Número da conta: ");
		System.out.println("Agência da conta: ");
		System.out.println("Chave PIX da conta: ");
		System.out.println("Saldo da conta: ");
	};
	
}
